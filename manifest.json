{
    "name": "Dompston Plugs In",
    "description": "Explore the wonders of the Dompston Cinematic Universe with this extension.",
    "author": "Beach Luigi",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "circus_tent.png",
        "48": "circus_tent.png",
        "128": "circus_tent.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "circus_tent.png",
            "48": "circus_tent.png",
            "128": "circus_tent.png"
        }
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js",
                "confetti.min.js"
            ],
            "css": [
                "bubble.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "bulb-horn-honk.mp3"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}